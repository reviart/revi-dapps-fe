# Privy App ID (required for Privy authentication)
VITE_PRIVY_APP_ID=your-app-id

# Solana cluster to connect to (e.g., 'mainnet-beta', 'devnet', or 'testnet')
VITE_SOLANA_CLUSTER=devnet

# Solana RPC URL for the selected cluster (e.g., https://api.devnet.solana.com)
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com

# Alchemy API key for Solana (optional, used for additional RPC endpoints)
VITE_ALCHEMY_API_KEY=your-alchemy-api-key

# Cloudflare Web Analytics Token (optional, default: empty string)
VITE_CF_WEB_ANALYTIC_TOKEN=your-cloudflare-web-analytics-token